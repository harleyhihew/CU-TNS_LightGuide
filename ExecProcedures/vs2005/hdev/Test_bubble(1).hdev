<?xml version="1.0" encoding="UTF-8"?>
<hdevelop file_version="1.1" halcon_version="13.0.1.1">
<procedure name="main">
<interface/>
<body>
<c as_id="image_acquisition" as_name="Image Acquisition 01" as_grp="[1,1]" as_ord="1">* Image Acquisition 01: Code generated by Image Acquisition 01</c>
<l>init_acq (Image, AcqHandle, Pointer, Type, Width, Height)</l>
<c></c>
<l>bubble (Image, GammaImage, Region, ImageReduced, Region1, ConnectedRegions, SelectedRegions, SelectedRegions1, ConnectedRegions1, CircleRegion, UsedThreshold, Area, Row, Column, i)</l>
</body>
<docu id="main">
<parameters/>
</docu>
</procedure>
<procedure name="FindBubble">
<interface>
<io>
<par name="ImageReduced" base_type="iconic" dimension="0"/>
</io>
<oo>
<par name="Region1" base_type="iconic" dimension="0"/>
<par name="ConnectedRegions" base_type="iconic" dimension="0"/>
<par name="SelectedRegions" base_type="iconic" dimension="0"/>
<par name="SelectedRegions1" base_type="iconic" dimension="0"/>
<par name="ConnectedRegions1" base_type="iconic" dimension="0"/>
</oo>
<oc>
<par name="UsedThreshold" base_type="ctrl" dimension="0"/>
<par name="Area" base_type="ctrl" dimension="0"/>
<par name="Row" base_type="ctrl" dimension="0"/>
<par name="Column" base_type="ctrl" dimension="0"/>
</oc>
</interface>
<body>
<l>binary_threshold (ImageReduced, Region1, 'max_separability', 'dark', UsedThreshold)</l>
<l>fill_up_shape (Region1, Region1, 'inner_circle', 0, 0.8)</l>
<l>connection(Region1, ConnectedRegions)</l>
<l>select_shape (ConnectedRegions, SelectedRegions, 'holes_num', 'and', 0.9018, 1)</l>
<l>select_shape (SelectedRegions, SelectedRegions1, 'circularity', 'and', 0.26927, 0.5)</l>
<l>connection (SelectedRegions1, ConnectedRegions1)</l>
<l>area_center(ConnectedRegions1, Area, Row, Column)</l>
<l>return ()</l>
</body>
<docu id="FindBubble">
<parameters>
<parameter id="Area"/>
<parameter id="Column"/>
<parameter id="ConnectedRegions"/>
<parameter id="ConnectedRegions1"/>
<parameter id="ImageReduced"/>
<parameter id="Region1"/>
<parameter id="Row"/>
<parameter id="SelectedRegions"/>
<parameter id="SelectedRegions1"/>
<parameter id="UsedThreshold"/>
</parameters>
</docu>
</procedure>
<procedure name="enhanceImage">
<interface>
<io>
<par name="Image" base_type="iconic" dimension="0"/>
</io>
<oo>
<par name="GammaImage" base_type="iconic" dimension="0"/>
<par name="Region" base_type="iconic" dimension="0"/>
<par name="ImageReduced" base_type="iconic" dimension="0"/>
</oo>
</interface>
<body>
<l>gamma_image (Image, GammaImage, 0.416667, 0.055, 0.0031308, 255, 'true')</l>
<l>threshold (GammaImage, Region, 34, 255)</l>
<l>reduce_domain (GammaImage, Region, ImageReduced)</l>
<l>return ()</l>
</body>
<docu id="enhanceImage">
<parameters>
<parameter id="GammaImage"/>
<parameter id="Image"/>
<parameter id="ImageReduced"/>
<parameter id="Region"/>
</parameters>
</docu>
</procedure>
<procedure name="drawCircles">
<interface>
<io>
<par name="Image" base_type="iconic" dimension="0"/>
</io>
<oo>
<par name="Circle" base_type="iconic" dimension="0"/>
</oo>
<ic>
<par name="Row" base_type="ctrl" dimension="0"/>
<par name="Column" base_type="ctrl" dimension="0"/>
</ic>
<oc>
<par name="i" base_type="ctrl" dimension="0"/>
</oc>
</interface>
<body>
<c>* Current graphics context settings:</c>
<l>dev_set_color ('red')</l>
<l>dev_set_draw ('margin')</l>
<l>dev_set_line_width (2)</l>
<l>dev_set_shape ('original')</l>
<l>dev_set_lut ('default')</l>
<l>dev_set_paint (['default'])</l>
<c>* End of graphics context settings</c>
<l>dev_clear_window()</l>
<c></c>
<l>dev_display(Image)</l>
<l>for i:=0 to |Row|-1 by 1</l>
<l>    gen_circle (Circle, Row[i], Column[i], 20)</l>
<l>    paint_region(Circle,Image,Image,255.0,'margin')</l>
<l>endfor</l>
<l>return ()</l>
</body>
<docu id="drawCircles">
<parameters>
<parameter id="Circle"/>
<parameter id="Column"/>
<parameter id="Image"/>
<parameter id="Row"/>
<parameter id="i"/>
</parameters>
</docu>
</procedure>
</hdevelop>
